<html lang="es">
<head>
  <meta charset="utf-8">

  <title>Retrocompatibles · Top 40</title>
  <meta name="description" content="Top 40 de videojuegos">
  <meta name="author" content="@jslakro">
  <meta name="keywords" content="videogames, games, tops"/>
  <meta name="subject" content="videogames">
  <meta name="language" content="ES">
  <meta name="robots" content="index,follow" />
  
  <link rel="stylesheet" href="styles.css">
  <link rel="icon" type="image/png" href="images/favicon-32x32.png" sizes="32x32" />
  <link rel="icon" type="image/png" href="images/favicon-16x16.png" sizes="16x16" />
</head>

<body>
  
<div class="wrapper">

	<header class="box">
    <div class="box-body">
		<picture>  
			<source media="(max-width: 680px)" srcset="images/banner_mobile.png">  
			<img class="banner" src="images/banner.png">
		</picture>
    </div>
	</header>

      
	<section class="columns wrap-collabsible">
		
		<div class="column box">
			<h2>Alex</h2>
			<div class="box-body">
				<image src="images/alex.png"></image>
				<input id="collapsible2" class="toggle2" type="checkbox">
				<label for="collapsible2" class="lbl-toggle"><mark>more</mark></label>
				<div class="collapsible2-content">
				{{#Alex}}
					<div class="content-inner">
						<h4>• {{Juego}}</h4>
						<p>{{Descripcion}}</p>
						<img class="game" src="{{Imagen}}"  />
						<span><a href="{{Attribution.Source}}">By {{#getAuthor}}{{Attribution.Author}}{{/getAuthor}}</a></span>
					</div>
				{{/Alex}}
				</div>
			  </div>
		</div>
	  
		<div class="column box">
			<h2>Nico</h2>
			<div class="box-body">
				<image src="images/nico.png"></image>
				<input id="collapsible3" class="toggle3" type="checkbox">
				<label for="collapsible3" class="lbl-toggle"><mark>more</mark></label>
				<div class="collapsible3-content">
				{{#Nico}}
					<div class="content-inner">
						<h4>• {{Juego}}</h4>
						<p>{{Descripcion}}</p>
						<img class="game" src="{{Imagen}}"  />
						<span><a href="{{Attribution.Source}}">By {{#getAuthor}}{{Attribution.Author}}{{/getAuthor}}</a></span>
					</div>
				{{/Nico}}
				</div>
			  </div>
		</div>
		
		<div class="column box">
			<h2>Pol</h2>
			<div class="box-body">
				<image src="images/pol.png"></image>
				<input id="collapsible" class="toggle" type="checkbox">
				<label for="collapsible" class="lbl-toggle"><mark>more</mark></label>
				<div class=" collapsible-content">
				{{#Pol}}
					<div class="content-inner">
						<h4>• {{Juego}}</h4>
						<p>{{Descripcion}}</p>
						<img class="game" src="{{Imagen}}"  />
						<span><a href="{{Attribution.Source}}">By {{#getAuthor}}{{Attribution.Author}}{{/getAuthor}}</a></span>
					</div>
				{{/Pol}}
				</div>
			  </div>
		</div>
	</section>
	<footer class="box turquoise text-dark">
    
	{{#Todos}}
	<h2>{{Juego}}</h2>    
	<div class="box-body">
		
		<p>{{Descripcion}}</p>
		<img class="game" src="{{Imagen}}"  />
		
	</div>
	{{/Todos}}
	</footer>
</div>
</body>
</html>